<%= semantic_form_for :data, url: { action: :import_users }, method: :post do |f| %>
  <%= f.inputs "Form" do %>
    <%= f.input :source, as: :file, label: "File", :hint => "Excel file" %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, as: :button, label: "import" %>
  <% end %>
<% end %>